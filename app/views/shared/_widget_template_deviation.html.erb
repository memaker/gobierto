<script type="text/html" id="widget-template-deviation">
<h2>{{deviation_heading}}</h2>
<div class="box">
  <div class="inner">
    <div class="pure-g ">
      <div class="pure-u-1 pure-u-md-1-2">
        <h1 title="{{deviation_heading}}">{{deviation_percentage}}%</h1>
      </div>
      <div class="pure-u-1 pure-u-md-1-2 vizz_cont">
        {{#I}}
          <div id="evo_line_income" class="vizz" data-series="{{evolution_to_s}}"></div>
        {{/I}}
        {{#G}}
          <div id="evo_line_expenditure" class="vizz" data-series="{{evolution_to_s}}"></div>
        {{/G}}
      </div>
    </div>
    <div class="totals">
      <table>
        <tr>
          <th>{{#I}}Ingresos{{/I}}{{#G}}Gastos{{/G}} presupuestados</th>
          <td>{{#I}}{{total_budgeted}}{{/I}}{{#G}}{{total_budgeted}}{{/G}}</td>
        </tr>
        <tr>
          <th>{{#I}}Ingresos{{/I}}{{#G}}Gastos{{/G}} reales</th>
          <td>{{#I}}{{total_executed}}{{/I}}{{#G}}{{total_executed}}{{/G}}</td>
        </tr>
      </table>
      <p>
        {{#I}}{{deviation_summary}}{{/I}}
        {{#G}}{{deviation_summary}}{{/G}}
      </p>
    </div>
  </div>
</div>
</script>