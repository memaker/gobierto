<script type="text/html" id="widget-template-deviation">
<h2 class="lite">{{deviation_heading}}</h2>
<div class="block">
  <div class="inner">
    <div class="pure-g ">
      <div class="pure-u-1 pure-u-md-1 pure-u-lg-1-2">
        <h1 title="{{deviation_heading}}">{{deviation_percentage}}%</h1>
      </div>
      <div class="pure-u-1 pure-u-md-1 pure-u-lg-1-2 vizz_cont">
        {{#I}}
          <div id="evo_line_income" class="vizz" data-series="{{evolution_to_s}}"></div>
        {{/I}}
        {{#G}}
          <div id="evo_line_expenditure" class="vizz" data-series="{{evolution_to_s}}"></div>
        {{/G}}
      </div>
    </div>
    <table>
    {{#I}}
      <tr>
        <th>Ingresos presupuestados</th>
        <td>{{total_budgeted}}</td>
      </tr>
      <tr>
        <th>Ingresos reales</th>
        <td>{{total_executed}}</td>
      </tr>
    {{/I}}
    {{#G}}
      <tr>
        <th>Gastos presupuestados</th>
        <td>{{total_budgeted}}</td>
      </tr>
      <tr>
        <th>Gastos reales</th>
        <td>{{total_executed}}</td>
      </tr>
    {{/G}}
    </table>
    <p>{{deviation_summary}}</p>
  </div>
</div>
</script>